.Parentlevel {
  width: 100%;
  height: 100%;
}
.Parentlevel .Waitfor {
  text-align: center;
  font-size: 1rem;
}
.Parentlevel .Headportrait {
  width: 130rpx;
  height: 130rpx;
  border-radius: 65rpx;
  margin: 15% auto;
  background-color: red;
}
.Parentlevel .Option {
  width: 100%;
  height: 50%;
  margin-top: 30%;
}
.Parentlevel .Option .userInfo {
  width: 100%;
  height: 100rpx;
  background-color: #5DBA9D;
  position: relative;
}
.Parentlevel .Option .userInfo .user {
  width: 50%;
  float: left;
}
.Parentlevel .Option .userInfo .user .userName {
  margin-left: 33%;
}
.Parentlevel .Option .userInfo .user .userIntegral {
  margin-left: 33%;
}
.Parentlevel .Option .P_hostile {
  width: 100%;
  height: 200rpx;
  margin-top: 30%;
  background-color: #5DBA9D;
  position: relative;
}
.Parentlevel .Option .P_hostile .P_GameplayerA {
  width: 130rpx;
  height: 130rpx;
  border-radius: 20rpx;
  position: absolute;
  margin: 8% 0 0 20%;
  background-color: coral;
}
.Parentlevel .Option .P_hostile .P_GameplayerA .username {
  margin-top: 80rpx;
  margin-left: 40rpx;
  color: #000;
}
.Parentlevel .Option .P_hostile .P_GameplayerVS {
  width: 130rpx;
  height: 130rpx;
  border-radius: 20rpx;
  position: absolute;
  margin: 8% 0 0 43%;
  text-align: center;
  line-height: 130rpx;
  font-size: 2rem;
}
.Parentlevel .Option .P_hostile .P_GameplayerB {
  width: 130rpx;
  height: 130rpx;
  border-radius: 20rpx;
  position: absolute;
  margin: 8% 0 0 65%;
  text-align: center;
  line-height: 130rpx;
  background-color: coral;
}
.Parentlevel .Option .P_hostile .P_Gsapn {
  font-size: 2rem;
}
.load_view {
  width: 130rpx;
  height: 130rpx;
}
.load_but {
  height: 34%;
  font-size: 35rpx;
  background-color: #5DBA9D;
  border-radius: 50px;
  color: #ECECEC;
  margin-top: 5%;
}

